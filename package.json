{
  "name": "appium-boilerplate",
  "version": "5.3.1",
  "description": "Boilerplate project to run WebdriverIO tests with Appium to test native applications on iOS and Android",
  "scripts": {
    "android9": "./node_modules/.bin/wdio ./config/wdio.android9.app.conf.js --cucumberOpts.tagExpression='@android'",
    "android10": "./node_modules/.bin/wdio ./config/wdio.android10.app.conf.js --cucumberOpts.tagExpression='@android'",
    "android11": "./node_modules/.bin/wdio ./config/wdio.android11.app.conf.js --cucumberOpts.tagExpression='@android'",
    "android:all": "npm run android9 && npm run android10 && npm run android11",
    "ios": "./node_modules/.bin/wdio ./config/wdio.ios.app.conf.js --cucumberOpts.tagExpression='@ios'",
    "test": "node ./test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webdriverio/appium-boilerplate.git"
  },
  "keywords": [
    "WebdriverIO",
    "WebDriver",
    "appium",
    "native app",
    "hybrid",
    "android",
    "ios"
  ],
  "author": "Wim Selles <wim@wswebcreation.nl>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/webdriverio/appium-boilerplate/issues"
  },
  "homepage": "https://github.com/webdriverio/appium-boilerplate#readme",
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-export-default-from": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/register": "^7.12.10",
    "@babel/traverse": "^7.12.12",
    "@babel/types": "^7.12.12",
    "@types/cucumber": "^6.0.1",
    "@wdio/appium-service": "^6.11.0",
    "@wdio/cli": "^6.12.0",
    "@wdio/config": "^6.11.0",
    "@wdio/cucumber-framework": "^6.11.1",
    "@wdio/jasmine-framework": "^6.11.0",
    "@wdio/local-runner": "^6.12.0",
    "@wdio/sauce-service": "^6.12.0",
    "@wdio/selenium-standalone-service": "^6.11.0",
    "@wdio/spec-reporter": "^6.11.0",
    "@wdio/static-server-service": "^6.10.10",
    "@wdio/sync": "^6.11.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "chromedriver": "^85.0.1",
    "eslint": "^7.18.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.1.0",
    "eslint-plugin-wdio": "^6.6.0",
    "http-server": "^0.12.1",
    "jest": "^26.6.3",
    "node-fetch": "^2.6.0",
    "npm-run-all": "^4.1.5",
    "wdio-chromedriver-service": "^6.0.4",
    "webdriverio": "^6.12.0"
  },
  "dependencies": {
    "appium-uiautomator2-driver": "^1.67.0",
    "appium-webdriveragent": "^2.35.1",
    "appium-xcuitest-driver": "^3.33.1",
    "chai": "^4.2.0",
    "concurrently": "^6.0.0"
  }
}
